{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-debugger": 0,
    "no-console": 1,
    "new-cap": 1,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "eol-last": 0,
    "eqeqeq": 2,
    "no-eval": 2,
    "no-var": 2,
    "semi": 2,
    "quotes": [
      2,
      "single"
    ]
  }
}